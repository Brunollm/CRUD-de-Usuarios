{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    //
    // Preencha abaixo com os seguintes parametros:
    //   --Server: Caminho do servidor que hospeda o SQL (ex: Server=NomeDoHost\\SQL)
    //   --Id: Login de acesso ao SQL Server
    //   --Password: Senha de Acesso ao SQL Server
    //   --Database: Nome do Bancco de Dados
    //
    // Fufill bellow with the following Parameters:
    //   --Server: Server Path that hosts the SQL (ex: Server=Hostname\\SQL)
    //   --Id: SQL Server Login user
    //   --Password: SQL Server access Password
    //   --Database: Database Name
    // 
    "DataBase": "Server=<ServerName>; User Id=<SqlId>; Password=<SqlPassword>; Database=<DataBase Name>; Trusted_Connection=True"
  },
  "AllowedHosts": "*"
}
